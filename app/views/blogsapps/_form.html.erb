<%= form_with(model: @blogsapp, local: true, url: choose_new_or_edit) do |form| %>
<% if @blogsapp.errors.any? %>
   <div id="error_explanation">
     <h2><%= @blogsapp.errors.count %>件のエラーがあります。</h2>
     <ul>
     <% @blogsapp.errors.full_messages.each do |msg| %>
       <li><%= msg %></li>
     <% end %>
     </ul>
   </div>
 <% end %>
  <div class="blogsapp_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="blogsapp_title">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>
