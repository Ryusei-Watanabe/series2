<h1>ブログ一覧</h1>
<p><%= notice %></p>
<table>
  <tr>
    <th>タイトル</th>
    <th>内容</th>
  </tr>
  <% @blogsapp.each do |blogsapp| %>
    <tr>
      <td><%= blogsapp.title %></td>
      <td><%= blogsapp.content %></td>
      <td><%= link_to '詳細を確認する', blogsapp_path(blogsapp.id) %></td>
      <td><%= link_to 'ブログを編集する', edit_blogsapp_path(blogsapp.id) %></td>
      <td><%= link_to 'ブログを削除する', blogsapp_path(blogsapp.id), method: :delete ,data: { confirm: '本当に削除していいですか？' } %></td>
    </tr>
  <% end %>
  </table>
  <br>
  <%= link_to '新しくブログを投稿する', new_blogsapp_path %>
